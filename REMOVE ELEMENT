int main() {
    int nums[] = {3, 2, 2, 3};
    int val = 3;
    int numsSize = 4;
    
    int k = removeElement(nums, numsSize, val);
    
    printf("k = %d\n", k); // Output: 2
    printf("First k elements: ");
    for (int i = 0; i < k; i++) {
        printf("%d ", nums[i]); // Output: 2 2
    }
    
    return 0;
}
