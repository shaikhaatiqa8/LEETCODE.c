#include <stdio.h>

int removeDuplicates(int* nums, int numsSize) {
    if (numsSize == 0) return 0;
    
    int k = 1; // Position for next unique element
    
    for (int i = 1; i < numsSize; i++) {
        if (nums[i] != nums[k - 1]) {
            nums[k] = nums[i];
            k++;
        }
    }
    
    return k;
}

// Test function
int main() {
    // Test case 1
    int nums1[] = {1, 1, 2};
    int size1 = sizeof(nums1) / sizeof(nums1[0]);
    int k1 = removeDuplicates(nums1, size1);
    printf("Test 1 - Unique elements: %d\n", k1);
    printf("Array after: ");
    for (int i = 0; i < k1; i++) {
        printf("%d ", nums1[i]);
    }
    printf("\n\n");
    
    // Test case 2
    int nums2[] = {0, 0, 1, 1, 1, 2, 2, 3, 3, 4};
    int size2 = sizeof(nums2) / sizeof(nums2[0]);
    int k2 = removeDuplicates(nums2, size2);
    printf("Test 2 - Unique elements: %d\n", k2);
    printf("Array after: ");
    for (int i = 0; i < k2; i++) {
        printf("%d ", nums2[i]);
    }
    printf("\n");
    
    return 0;
}
